{
  "name": "ecommerce_backend_products_service",
  "version": "1.0.0",
  "description": "Products Endpoints",
  "main": "app.js",
  "repository": "https://github.com/awslabs/aws-sam-cli/tree/develop/samcli/local/init/templates/cookiecutter-aws-sam-hello-nodejs",
  "author": "SAM CLI",
  "license": "MIT",
  "scripts": {
    "start": "sam local start-api --env-vars .env.prod.json",
    "start-stage": "sam local start-api --env-vars .env.stage.json",
    "build": "rm -rf ./.aws-sam build && webpack",
    "start-watch": "nodemon --watch build --exec 'sam local start-api --env-vars .env.prod.json'",
    "start-stage-watch": "nodemon --watch build --exec 'sam local start-api --env-vars .env.stage.json'",
    "start-prod-watch": "nodemon --watch build --exec 'sam local start-api --env-vars .env.prod.json'",
    "build-watch": "rm -rf ./.aws-sam build && webpack --watch",
    "unit": "jest",
    "lint": "eslint '*.ts' --quiet --fix",
    "compile": "tsc",
    "test": "npm run compile && npm run unit",
    "deploy-stage": "sam build && sam deploy -t ./.aws-sam/build/template.yaml --stack-name ecommerce-stage-backend-products-service --resolve-s3 --capabilities CAPABILITY_IAM --region me-central-1 --parameter-overrides EnvironmentParameter=Stage",
    "deploy-prod": "sam build && sam deploy -t ./.aws-sam/build/template.yaml --stack-name ecommerce-prod-backend-products2-service --resolve-s3 --capabilities CAPABILITY_IAM --region me-central-1 --parameter-overrides EnvironmentParameter=Prod"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.507.0",
    "@elastic/elasticsearch": "^8.12.1",
    "@hapi/joi": "^17.1.1",
    "@middy/core": "^4.6.5",
    "@middy/http-error-handler": "^4.6.5",
    "@middy/http-json-body-parser": "^4.6.5",
    "@middy/validator": "^5.1.0",
    "@mongodb-js/zstd": "^1.2.0",
    "algoliasearch": "^4.20.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "esbuild": "^0.14.54",
    "gcp-metadata": "^5.3.0",
    "kerberos": "^2.1.0",
    "ioredis": "^5.3.2",
    "middy": "^0.36.0",
    "mongodb": "^6.2.0",
    "mongodb-client-encryption": "^6.0.0",
    "redis": "^4.6.12",
    "socks": "^2.7.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/cors": "^2.8.16",
    "@types/hapi__joi": "^17.1.13",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.1",
    "nodemon": "^3.0.2",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
